{
  "name": "osi4kjb-stripe-add-billing-address",
  "version": "0.0.7",
  "description": "",
  "author": "Jason Go",
  "license": "Apache-2.0",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/saba.mjs",
  "main": "dist/saba.mjs",
  "browser": "dist/saba.min.js",
  "jsdelivr": "dist/saba.min.js",
  "unpkg": "dist/saba.min.js",
  "exports": {
    "import": "./dist/saba.mjs",
    "default": "./dist/saba.mjs",
    "types": "./src/index.d.ts"
  },
  "scripts": {
    "lint": "ts-standard",
    "format": "prettier --ignore-path src/patch.ts --check src/**",
    "patch": "tsx build/createPatch.ts",
    "build:reset": "rimraf ./dist",
    "build:esm": "esbuild src/index.ts --bundle --packages=external --legal-comments=inline --banner:js='/*! Copyright 2023 Jason Go - Apache-2.0 */' --target=es2015 --format=esm --outfile=dist/saba.mjs",
    "build:iife": "esbuild src/iife.ts --bundle --minify --sourcemap=linked --legal-comments=inline --banner:js='/*! Copyright 2023 Jason Go - Apache-2.0 */' --target=es2015 --format=iife --outfile=dist/saba.min.js",
    "build": "run-p lint format build:reset && run-p build:esm build:iife"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.4"
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-standard": "^12.0.2"
  },
  "keywords": [
    "kajabi",
    "stripe",
    "checkout",
    "address",
    "billing",
    "osi4kjb"
  ],
  "ts-standard": {
    "ignore": [
      "node_modules/**",
      "dist/**",
      "*.d.ts"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 160,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  }
}
